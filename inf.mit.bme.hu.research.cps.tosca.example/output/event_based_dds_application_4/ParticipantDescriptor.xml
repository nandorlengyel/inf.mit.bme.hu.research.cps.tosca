<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="6.0.0" xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.0/rti_dds_profiles.xsd">
    <qos_library name="QosLibrary">
        <qos_profile base_name="BuiltinQosLib::Generic.StrictReliable" is_default_qos="true" name="DefaultProfile">
            <participant_qos>
                <participant_name>
                    <name>event_based_dds_application_4</name>
                </participant_name>
            </participant_qos>
        </qos_profile>
    </qos_library>
    <types>
        <struct extensibility="extensible" name="RadarData">
            <member name="isOccupied" type="boolean"/>
            <member name="readTime" type="float"/>
            <member name="radarId" stringMaxLength="30" type="string"/>
        </struct>
        <enum extensibility="extensible" name="VehicleType">
            <enumerator name="BIKE" value="0"/>
            <enumerator name="CAR" value="1"/>
            <enumerator name="BUS" value="2"/>
            <enumerator name="CONVOY" value="3"/>
            <enumerator name="PARAGLIDER" value="4"/>
        </enum>
        <struct extensibility="extensible" name="Pos">
            <member name="x" stringMaxLength="30" type="string"/>
            <member name="y" stringMaxLength="30" type="string"/>
        </struct>
        <struct extensibility="extensible" name="crossingOccupancy">
            <member name="licensePlate" stringMaxLength="30" type="string"/>
            <member name="detectionPos" nonBasicTypeName="Pos" type="nonBasic"/>
            <member name="detectionTime" type="float"/>
            <member name="speed" type="float"/>
            <member name="vehicleType" nonBasicTypeName="VehicleType" type="nonBasic"/>
        </struct>
    </types>
    <domain_library name="DDSDomainLibrary">
        <domain domain_id="0" name="DDSDomain">
            <register_type name="RadarData" type_ref="RadarData"/>
            <topic name="dds_topic_2" register_type_ref="RadarData"/>
            <register_type name="crossingOccupancy" type_ref="crossingOccupancy"/>
            <topic name="dds_topic_8" register_type_ref="crossingOccupancy"/>
        </domain>
    </domain_library>
    <domain_participant_library name="event_based_dds_application_4ParticipantLibrary">
        <domain_participant domain_ref="DDSDomainLibrary::DDSDomain" name="event_based_dds_application_4Participant">
            <publisher name="dds_topic_8Publisher">
                <data_writer name="dds_topic_8Writer" topic_ref="dds_topic_8"/>
            </publisher>
            <subscriber name="dds_topic_2Subscriber">
                <data_reader name="dds_topic_2Reader" topic_ref="dds_topic_2"/>
            </subscriber>
        </domain_participant>
    </domain_participant_library>
</dds>
