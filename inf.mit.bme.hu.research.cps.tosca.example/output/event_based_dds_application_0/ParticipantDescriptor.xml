        <domain_participant domain_ref="DDSDomainLibrary::DDSDomain" name="event_based_dds_application_0Participant">
            <publisher name="dds_topic_0Publisher">
                <data_writer name="dds_topic_0Writer" topic_ref="dds_topic_0"/>
            </publisher>
            <subscriber name="dds_topic_12Subscriber">
                <data_reader name="dds_topic_12Reader" topic_ref="dds_topic_12"/>
            </subscriber>
        </domain_participant>
    </domain_participant_library>
</dds>
