<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="6.0.0" xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.0/rti_dds_profiles.xsd">
    <qos_library name="QosLibrary">
        <qos_profile base_name="BuiltinQosLib::Generic.StrictReliable" is_default_qos="true" name="DefaultProfile">
            <participant_qos>
                <participant_name>
                    <name>event_based_dds_application_0</name>
                </participant_name>
            </participant_qos>
        </qos_profile>
    </qos_library>
    <types>
        <enum extensibility="extensible" name="RailwayAlertsType">
            <enumerator name="SLOW" value="0"/>
            <enumerator name="STOP" value="1"/>
            <enumerator name="EMERGENCYSTOP" value="2"/>
        </enum>
        <struct extensibility="extensible" name="RailwayAlerts">
