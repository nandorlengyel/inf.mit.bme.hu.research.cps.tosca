        <enum extensibility="extensible" name="RiskLevel">
            <enumerator name="LOW" value="0"/>
            <enumerator name="MID" value="1"/>
            <enumerator name="HIGH" value="2"/>
            <enumerator name="EMERGENCY" value="3"/>
        </enum>
        <struct extensibility="extensible" name="RiskLevelAssesment">
            <member name="riskLevel" nonBasicTypeName="RiskLevel" type="nonBasic"/>
            <member name="issuedTime" type="float"/>
            <member name="validDuration" type="float"/>
            <member name="riskID" stringMaxLength="30" type="string"/>
        </struct>
    </types>
    <domain_library name="DDSDomainLibrary">
        <domain domain_id="0" name="DDSDomain">
            <register_type name="RailwayAlerts" type_ref="RailwayAlerts"/>
            <topic name="dds_topic_0" register_type_ref="RailwayAlerts"/>
            <register_type name="RiskLevelAssesment" type_ref="RiskLevelAssesment"/>
            <topic name="dds_topic_12" register_type_ref="RiskLevelAssesment"/>
