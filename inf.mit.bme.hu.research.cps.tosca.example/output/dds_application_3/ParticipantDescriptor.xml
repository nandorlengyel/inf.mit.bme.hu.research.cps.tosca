<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="6.0.0" xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.0/rti_dds_profiles.xsd">
    <qos_library name="QosLibrary">
        <qos_profile base_name="BuiltinQosLib::Generic.StrictReliable" is_default_qos="true" name="DefaultProfile">
            <participant_qos>
                <participant_name>
                    <name>dds_application_3</name>
                </participant_name>
            </participant_qos>
        </qos_profile>
    </qos_library>
    <types>
        <struct extensibility="extensible" name="Pos">
            <member name="x" stringMaxLength="30" type="string"/>
            <member name="y" stringMaxLength="30" type="string"/>
        </struct>
        <struct extensibility="extensible" name="VehicleTelemetry">
            <member name="licensePlate" stringMaxLength="30" type="string"/>
            <member name="telemetryTime" type="float"/>
            <member name="detectionPos" nonBasicTypeName="Pos" type="nonBasic"/>
            <member name="telemetryID" stringMaxLength="30" type="string"/>
            <member name="speed" type="float"/>
        </struct>
    </types>
    <domain_library name="DDSDomainLibrary">
        <domain domain_id="0" name="DDSDomain">
            <register_type name="VehicleTelemetry" type_ref="VehicleTelemetry"/>
            <topic name="dds_topic_5" register_type_ref="VehicleTelemetry"/>
        </domain>
    </domain_library>
    <domain_participant_library name="dds_application_3ParticipantLibrary">
        <domain_participant domain_ref="DDSDomainLibrary::DDSDomain" name="dds_application_3Participant">
            <publisher name="dds_topic_5Publisher">
                <data_writer name="dds_topic_5Writer" topic_ref="dds_topic_5"/>
            </publisher>
        </domain_participant>
    </domain_participant_library>
</dds>
